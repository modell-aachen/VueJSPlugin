%TMPL:DEF{"commentbar"}%<!--commentbar-->
<div class="qw-commentbar" data-offcanvas data-target="commentbar">
  <div>
    <div class="header">
      <div class="default">
        <span>%MAKETEXT{"Comments"}%</span>
        <a href="#" class="close" data-offcanvas-toggle="commentbar"></a>
      </div>
      <div class="idea">
        <i class="icon-comment-idea"></i>
        <span>%MAKETEXT{"Idea"}%</span>
        <a href="#" class="close right" data-offcanvas-toggle="commentbar"></a>
      </div>
      <div class="issue">
        <i class="icon-exclamation"></i>
        <span>%MAKETEXT{"Issue"}%</span>
        <a href="#" class="close right" data-offcanvas-toggle="commentbar"></a>
      </div>
    </div>
    <div class="content">
      <div data-scrollcontainer>
        <div class="editbox">%TMPL:P{"comments:editbox"}%</div>
        <div class="viewbox">%TMPL:P{"comments:viewbox"}%</div>
        <div class="replybox">%TMPL:P{"comments:replybox"}%</div>
      </div>
    </div>
  </div>
</div>
<!--/commentbar-->%TMPL:END%


%TMPL:DEF{"leftbar"}%<!--leftbar-->
<nav class="qw-leftbar" data-offcanvas data-friendly data-target="leftbar">
  <section class="buttons">%TMPL:P{"leftbar:buttons"}%</section>
  <section class="menu">
    <div class="links">%TMPL:P{"leftbar:links"}%</div>
    <div class="user">
      <div>
        <span>%MAKETEXT{"Logged in as:"}%</span>
        <span>%WIKINAME%</span>
      </div>
    </div>
    <div class="actions">
      <div>
        <a href="%SCRIPTURLPATH{"view"}%/Manuals" class="qw-prio3-btn"><i class="icon-questionmark-circle"></i></a>
        <a href="%IF{"$ USERNAME != 'guest'" then='%SCRIPTURLPATH{"view"}%?logout=1' else='%SCRIPTURLPATH{"login"}%'}%" class="qw-prio3-btn" title="%IF{"$ USERNAME != 'guest'" then='%MAKETEXT{"Logout"}%' else='%MAKETEXT{"Login"}%'}%">
        <i class="icon-%IF{"$ USERNAME != 'guest'" then='logout' else='login'}%"></i>
        </a>
        <a href="%SCRIPTURLPATH{"view"}%/%WEB%/WebPreferences" class="qw-prio3-btn"><i class="icon-config"></i></a>
      </div>
    </div>
    <div class="ma-logo">
      <span>Modell Aachen GmbH</span>
    </div>
  </section>
</nav>
<!--/leftbar-->%TMPL:END%


%TMPL:DEF{"infobar"}%<!--infobar-->
<div class="qw-infobar" data-offcanvas data-offcanvas-persist-state data-target="infobar">
  <div>
    <div class="header">
      <div>
        <i class="icon-page-info"></i>
        <span>%MAKETEXT{"Document information"}%</span>
        <a href="#" class="qw-close-btn" data-maximize-toggle=".qw-page, .qw-top .two .actions"></a>
      </div>
    </div>
    <div class="content">
      <div>
        <span class="label">%MAKETEXT{"General"}%</span>
        <ul>
          <li><a href="#" data-attachtable-toggle>%MAKETEXT{"Attachments"}%</a></li>
          <li><a href="#" data-offcanvas-toggle="documentcontrol">%MAKETEXT{"Document control"}%</a></li>
          <li><a href="#">%MAKETEXT{"Version history"}%</a></li>
          <li><a href="#">%MAKETEXT{"Key figures"}%</a></li>
          <li><a href="#">%MAKETEXT{"Language comparison"}%</a></li>
        </ul>
      </div>
      <div>
        <span class="label">%MAKETEXT{"KVP"}%</span>
        <ul>
          <li><a href="#">%MAKETEXT{"Tasks"}%</a></li>
          <li><a href="#">%MAKETEXT{"Actions"}%</a></li>
          <li><a href="#">%MAKETEXT{"Risks"}%</a></li>
        </ul>
      </div>
    </div>
  </div>
</div>
<!--/infobar-->%TMPL:END%

%TMPL:DEF{"menutoggle"}%<!--menutoggle-->
<div class="qw-menutoggle" data-offcanvas>
  <div class="customer-logo">%TMPL:P{"logo"}%</div>
  <div class="toggle" data-offcanvas-toggle="leftbar"><div><i class="icon-menu"></i></div></div>
</div>
<!--/menutoggle-->%TMPL:END%


%TMPL:DEF{"breadcrumb"}%<!--breadcrumb-->
<ul class="qw-breadcrumb" data-breadcrumb>
  <!--breadcrumb:home-->%TMPL:P{"breadcrumb:home"}%<!--/breadcrumb:home-->
  <!--breadcrumb:parentwebs-->%TMPL:P{"breadcrumb:parentwebs"}%<!--/breadcrumb:parentwebs-->
  <!--breadcrumb:parenttopics-->%TMPL:P{"breadcrumb:parenttopics"}%<!--/breadcrumb:parenttopics-->
  <!--breadcrumb:topic-->%TMPL:P{"breadcrumb:topic"}%<!--/breadcrumb:topic-->
</ul>
<!--/breadcrumb-->%TMPL:END%

%TMPL:DEF{"breadcrumb:home"}%<li class="qw-breadcrumb-home">[[%USERSWEB%/%HOMETOPIC%][<i class="icon-app-processes"></i>]]</li>%TMPL:END%
%TMPL:DEF{"breadcrumb:parentwebs"}%%MAWEBLIST%%TMPL:END%
%TMPL:DEF{"breadcrumb:parenttopics"}%%META{"parent" nowebhome="on" prefix="<li class=\"qw-breadcrumb-parent\">" separator="</li><li class=\"qw-breadcrumb-parent\">" suffix="</li>"}%%TMPL:END%
%TMPL:DEF{"breadcrumb:topic"}%%IF{"'%TOPIC%' != 'WebHome'" then="<li class=\"qw-breadcrumb-topic\">[[%TOPIC%][$percntTMPL:P{\"breadcrumb:topic:title\"}$percnt]]</li>" else="<li class=\"qw-breadcrumb-topic\">[[%TOPIC%][TBD. WebHome Text]]</li>"}%%TMPL:END%
%TMPL:DEF{"breadcrumb:topic:title"}%%IF{"'%TOPIC%'/TopicTitle" then="$percntFORMFIELD{\"TopicTitle\"}$percnt" else="$percntTOPIC$percnt"}%%TMPL:END%

%TMPL:DEF{"attachments"}%%META{"attachments"}%%TMPL:END%


%TMPL:DEF{"comments"}%<!--comments-->
%TMPL:P{"comments:adorner"}%
%TMPL:P{"comments:container"}%
<!--/comments-->%TMPL:END%

%TMPL:DEF{"comments:adorner"}%<!--comments:adorner-->
<div id="qw-comment-adorner"><i class="icon-add"></i></div>
<!--/comments:adorner-->%TMPL:END%

%TMPL:DEF{"comments:container"}%<!--comments:container-->
<div id="qw-comment-container">%FLATCOMMENTS%</div>
<!--/comments:container-->%TMPL:END%

%TMPL:DEF{"comments:viewbox"}%<!--comments:viewbox-->
<div class="qw-comment-viewbox">
  <div>
    <div class="entry" data-qtemplate="qw.comments">
      <div>
        <div class="comment">
          <div class="content">
            <div qt-bind="text"></div>
          </div>
        </div>
        <div class="meta">
          <div class="content">
            <div>
              <span class="qw-meta-author" qt-bind="author"></span>
            </div>
          </div>
          <div class="content">
            <div>
              <span class="qw-meta-date right" qt-bind="dateString"></span>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!--/comments:viewbox-->%TMPL:END%

%TMPL:DEF{"comments:replybox"}%<!--comments:replybox-->
<div class="qw-comment-reply">
  <div class="btn">
    <a class="qw-prio1-btn reply-btn" href="#">%MAKETEXT{"comment"}%</a>
  </div>
  <div class="editor">
    <div class="text">
      <div class="content">
        <form data-abide>
          <div>
            <textarea id="qw-reply-textarea" cols="30" rows="10" required pattern="^[^\s]$|.+"></textarea>
            <small class="error">%MAKETEXT{"required"}%</small>
          </div>
        </form>
      </div>
    </div>
    <div class="actions">
      <div class="content">
        <div><a href="#" class="qw-prio1-btn stretch submit">%MAKETEXT{"save"}%</a></div>
      </div>
      <div class="content">
        <div><a href="#" class="qw-prio2-btn stretch cancel">%MAKETEXT{"cancel"}%</a></div>
      </div>
    </div>
  </div>
</div>
<!--/comments:replybox-->%TMPL:END%

%TMPL:DEF{"comments:editbox"}%<!--comments:editbox-->
<div class="qw-comment-editbox">
  <div class="options">
    <div class="content">
      <input type="radio" name="commentType" value="0" id="qw-cmt-type-idea"><label for="qw-cmt-type-idea"><span class="idea"><i class="icon-comment-idea"></i> %MAKETEXT{"idea"}%</span></label>
    </div>
    <div class="content">
      <input type="radio" name="commentType" value="1" id="qw-cmt-type-issue"><label for="qw-cmt-type-issue"><span class="issue"><i class="icon-exclamation"></i> %MAKETEXT{"issue"}%</span></label>
    </div>
  </div>
  <div class="text">
    <div class="content">
      <form data-abide>
        <div>
          <textarea id="qw-comment-textarea" cols="30" rows="10" required pattern="^[^\s]$|.+"></textarea>
          <small class="error">%MAKETEXT{"required"}%</small>
        </div>
      </form>
    </div>
  </div>
  <div class="actions">
    <div class="content">
      <div><a href="#" class="qw-prio1-btn stretch submit">%MAKETEXT{"save"}%</a></div>
    </div>
    <div class="content">
      <div><a href="#" class="qw-prio2-btn stretch cancel">%MAKETEXT{"cancel"}%</a></div>
    </div>
  </div>
</div>
<!--/comments:editbox-->%TMPL:END%




