%{
  Prints the entire document.
  (wrapped in comments to prevent foswiki's theme engine to produce unwanted paragraphs)
}%
%{}%%TMPL:P{"document"}%%{}%

%{ HTML5 doctype }%
%TMPL:DEF{"doctype"}%<!DOCTYPE html>%TMPL:END%
%TMPL:DEF{"htmlstart"}%<html class="no-js" lang="%LANG%">%TMPL:END%
%TMPL:DEF{"htmlend"}%</html>%TMPL:END%

%TMPL:DEF{"head"}%%{
}%<head>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="%TOPIC%">
  <meta name="TEXT_NUM_TOPICS" content="%MAKETEXT{"Number of topics:"}%">
  <meta name="TEXT_MODIFY_SEARCH" content="%MAKETEXT{"Modify search"}%">
  <meta name="robots" content="noindex">
  <link rel="icon" href="%FAVICON%" type="image/x-icon" />
  <link rel="shortcut icon" href="%FAVICON%" type="image/x-icon" />
  <link rel="alternate" href="%SCRIPTURL{edit}%/%BASEWEB%/%BASETOPIC%?t=%GMTIME{"$epoch"}%" type="application/x-wiki" title="edit %BASETOPIC%" />
  %TMPL:P{"script"}%
  %TMPL:P{"windowtitle"}%
  %TMPL:P{"defaultstyle"}%
  %TMPL:P{"JavascriptFiles/foswikiPref"}%
</head>%TMPL:END%

%TMPL:DEF{"context"}%approved%TMPL:END%

%TMPL:DEF{"offcanvas:bars"}%<!--offcanvas:bars-->
%TMPL:P{"commentbar"}%
%TMPL:P{"comments"}%
<!--/offcanvas:bars-->%TMPL:END%

%TMPL:DEF{"body"}%<!--body-->
<body class="context-%TMPL:P{"context"}%">%JQREQUIRE{"blockui, cookie, placeholder, ui::autocomplete"}%
%TMPL:P{"topbar"}%
%TMPL:P{"menutoggle"}%
<!-- offcanvas:bars -->%TMPL:P{"offcanvas:bars"}%<!-- /offcanvas:bars -->
<div class="qw-page" data-offcanvas>
  <section class="heading"><div>%TMPL:P{"document:head"}%</div></section>
  <section class="content"><div>%TMPL:P{"content"}%</div></section>
</div>
<script src="%PUBURLPATH%/%SYSTEMWEB%/QgularContrib/qgular.js"></script>
<script>
jQuery(function($) {
  $(this).qwiki();
});
</script>
</body>
<!--/body-->%TMPL:END%


%TMPL:DEF{"topbar"}%<!--topbar-->
<nav class="qw-top" data-offcanvas>
  <div class="one">
    <div class="breadcrumb"><div>%TMPL:P{"breadcrumb"}%</div></div>
    <div class="search">
      <div class="close" data-offcanvas-toggle="quicksearch">
        <div><i class="icon-arrow-right"></i></div>
      </div>
      <div>%TMPL:P{"topbar:search"}%</div>
    </div>
  </div>
  <div class="two">
    <div class="actions">
      <div>
        <div>
          <div class="tabs">
            <div>%TMPL:P{"topbar:tabs"}%</div>
          </div>
        </div>
        <div>
          <div class="topicactions">
            <div>%TMPL:P{"topbar:actions"}%</div>
          </div>
        </div>
      </div>
    </div>
  </div>
</nav>
<!--/topbar-->%TMPL:END%


%TMPL:DEF{"topbar:search"}%<!--topbar:search-->
<div class="qw-quicksearch-form">
  <form class="form" action="%SCRIPTURLPATH{view}%/%WEB%/WebSearch">
<!--
TODO
    <div class="toggle">
      <div id="qw-quicksearch-filter-toggle" class="filter-toggle">
        <span class="filter-show">%MAKETEXT{"Filters"}% <i class="fa fa-chevron-down"></i></span>
        <span class="filter-hide">%MAKETEXT{"Filters"}% <i class="fa fa-chevron-up"></i></span>
      </div>
    </div>
-->
    <div class="input">
      <input name="search"
             type="text"
             class="jqUIAutocomplete {source:'%SCRIPTURL%/rest/SolrPlugin/autocomplete?limit=5', delay: 500}"
             placeholder="%MAKETEXT{"search..."}%"
             data-offcanvas-toggle="quicksearch">
    </div>
    <div class="submit">
      <a href="#" class="submit"><i class="icon-search"></i></a>
    </div>
  </form>
</div>
<div class="qw-search-toggle">
  <a href="#" data-offcanvas-toggle="quicksearch"><i class="icon-search"></i></a>
</div>
<!--/topbar:search-->%TMPL:END%

