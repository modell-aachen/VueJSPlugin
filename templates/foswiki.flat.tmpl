%TMPL:DEF{"topbar:one"}%
<nav class="qw-top-one">
  <div class="qw-menu">
    <a href="#" class="button tiny alert" data-offcanvas-toggle data-target="#qw-leftbar">%MAKETEXT{"Menu"}%</a>
  </div>
  <div class="qw-breadcrumbs">
    %TMPL:P{"breadcrumb"}%
  </div>
  <div class="qw-search">
    <form>
      <div class="row collapse">
        <div class="small-10 columns">
          <input type="text" placeholder="%MAKETEXT{"Search"}%" style="height: 34px" data-offcanvas-toggle data-right data-target="#bar">
        </div>
        <div class="small-2 columns">
          <a href="#" class="button tiny postfix" style="height: 33px"><i class="fa fa-search"></i></a>
        </div>
      </div>
    </form>
  </div>
</nav>
