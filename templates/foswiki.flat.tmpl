  <div class="results" data-scrollcontainer>
      <div class="qw-total-results" data-qtemplate="total">
        <span class="left"><span qbind="count"></span> results</span>
        <a href="#" class="right" data-quicksearch-show-all>%MAKETEXT{"show all"}%</a>
        <span class="clearfix"></span>
      </div>
      <div class="qw-no-results" data-qtemplate="noresults"><h3 class="left">%MAKETEXT{"No results"}%</h3></div>
      <div class="qw-search-result" data-qtemplate qbind="data-url:url">
        <div class="content">
          <div class="preview" data-open-preview>
            <a href="#" class="button qw-prio1-btn" data-close-text="%MAKETEXT{"close"}%" data-open-preview>%MAKETEXT{"Preview"}%</a>
          </div>
          <div class="title" qbind="data-draft: draft, i // class: icon, span // title"><i></i><span></span></div>
          <div class="snippet" qbind="snippet"></div>
        </div>
      </div>
  </div>
</div>
<section id="qw-search-pageoverlay" data-offcanvas>
  <div class="overlay-top"></div>
</section>
<section id="qw-searchpreview">
  <div class="title">
    <div class="close">
      <div class="qw-back-btn forward primary" data-close-preview></div>
    </div>
    <div class="heading">
    </div>
  </div>
  <div class="content"></div>
</section>
