%TMPL:DEF{"topbar:one"}%
<div class="qw-top-one">
  <div class="qw-menu">
    <a href="#" class="button tiny secondary" data-offcanvas-toggle data-target="#qw-leftbar">%MAKETEXT{"Menu"}%</a>
  </div>
  <div class="qw-breadcrumbs"></div>
  <div class="qw-search">
    <a href="#" class="button tiny right" data-offcanvas-toggle data-right data-target="#bar">TR</a>
