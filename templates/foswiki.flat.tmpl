%TMPL:DEF{"content:head"}%<!--content:head-->
%TMPL:P{"document:head"}%
<!--/content:head-->%TMPL:END%

%TMPL:DEF{"body:content"}%
<div class="qw-page follows-canvas" data-offcanvas>
  <section class="qw-left">
    <article class="meta">%TMPL:P{"content:head"}%</article>
    <article class="content">%TMPL:P{"content"}%</article>
  </section>
  <section class="qw-right">
    <article class="content">
      <div class="qw-comment-viewbox">
        <div class="meta">
          <div class="content">
            <span class="user"></span>
          </div>
          <div class="content">
            <span class="date"></span>
          </div>
        </div>
        <div class="comment">
          <div class="content"></div>
        </div>
        <div class="more">
          <div class="content">
            <a id="qw-comment-more" href="#">%MAKETEXT{"read more"}% <i class="fa fa-chevron-down"></i></a>
            <a id="qw-comment-less" class="hidden" href="#">%MAKETEXT{"less"}% <i class="fa fa-chevron-up"></i></a>
          </div>
        </div>
      </div>
      <div class="qw-comment-editbox">
        <div class="options">
          <div class="content">
            <input type="radio" name="commentType" value="0" id="qw-cmt-type0"><label for="qw-cmt-type0"><span class="idea left"><i class="fa fa-lightbulb-o fa-lg"></i> %MAKETEXT{"idea"}%</span></label>
          </div>
          <div class="content">
            <input type="radio" name="commentType" value="1" id="qw-cmt-type1"><label for="qw-cmt-type1"><span class="neutral"><i class="fa fa-align-left fa-lg"></i> %MAKETEXT{"neutral"}%</span></label>
          </div>
          <div class="content">
            <input type="radio" name="commentType" value="2" id="qw-cmt-type2"><label for="qw-cmt-type2"><span class="issue right"><i class="fa fa-exclamation fa-lg"></i> %MAKETEXT{"issue"}%</span></label>
          </div>
        </div>
        <div class="text">
          <div class="content">
            <form data-abide>
              <div>
                <textarea id="qw-comment-textarea" cols="30" rows="5" required pattern="^[^\s]$|.+"></textarea>
                <small class="error">%MAKETEXT{"required"}%</small>
              </div>
            </form>
          </div>
        </div>
        <div class="actions">
          <div class="content">
            <a href="#" class="submit">%MAKETEXT{"save"}%</a>
          </div>
          <div class="content">
            <a href="#" class="cancel">%MAKETEXT{"cancel"}%</a>
          </div>
        </div>
      </div>
    </article>
  </section>
</div>
%TMPL:END%

%TMPL:DEF{"topbar"}%<!--topbar:one-->%TMPL:P{"topbar:one"}%<!--/topbar:one--><!--topbar:two-->%TMPL:P{"topbar:two"}%<!--/topbar:two-->%TMPL:END%

%TMPL:DEF{"topbar:one:left"}%<!--topbar:one:menu-->
<div class="qw-menu" data-offcanvas-toggle data-target="#qw-offcanvas-leftbar" data-type="leftbar" data-activate="#qw-menu-btn">
  <a id="qw-menu-btn" href="#" class="qw-menu-btn"><span class="text">&nbsp;%MAKETEXT{"Menu"}%</span></a>
</div>
<!--/topbar:one:menu-->%TMPL:END%

%TMPL:DEF{"topbar:one:mid"}%<!--topbar:one:breadcrumbs-->
<div class="qw-breadcrumbs"><!--breadcrumb-->%TMPL:P{"breadcrumb"}%<!--/breadcrumb--></div>
<!--/topbar:one:breadcrumbs-->%TMPL:END%

%TMPL:DEF{"topbar:one:right"}%<!--topbar:one:right-->
<div class="qw-search">
  <div class="close">
    <div class="qw-close-btn" title="%MAKETEXT{"Close"}%" data-offcanvas-toggle data-target="#qw-searchbar" data-type="search" data-right data-close-preview></div>
  </div>
  <span><i class="qw-search-icon" data-offcanvas-toggle data-offcanvas-autoclose="0" data-quicksearch-toggle data-target="#qw-searchbar" data-type="search" data-trigger="focus" data-right></i></span>
  <form action="%SCRIPTURLPATH{view}%/%WEB%/WebSearch" class="qw-search-form">
    <div class="form-content">
      <div class="qw-inline-input">%JQREQUIRE{"UI::autocomplete"}%
        <input name="search"
               type="text"
               class="jqUIAutocomplete {source:'%SCRIPTURL%/rest/SolrPlugin/autocomplete?limit=5', delay: 500}"
               placeholder="%MAKETEXT{"search..."}%"
               data-offcanvas-toggle
               data-offcanvas-autoclose="0"
               data-quicksearch-toggle
               data-target="#qw-searchbar"
               data-type="search"
               data-right
               required>
      </div>
      <div class="qw-inline-submit">
        <a href="#" class="submit" style="padding: 10px;"><i class="fa fa-lg fa-search"></i></a>
      </div>
    </div>
  </form>
</div>
<!--/topbar:one:right-->%TMPL:END%

%TMPL:DEF{"topbar:one"}%<!--topbar:one-->
<nav class="qw-top-one">
  <!--topbar:one:left-->%TMPL:P{"topbar:one:left"}%<!--/topbar:one:left-->
