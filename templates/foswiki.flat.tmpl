%TMPL:INCLUDE{"TopbarButtons"}%
%TMPL:INCLUDE{"DocumentHead"}%
%TMPL:INCLUDE{"LeftBar"}%

%TMPL:DEF{"document"}%
%TMPL:P{"doctype"}%
%TMPL:P{"htmlstart"}%
%TMPL:P{"head"}%
%TMPL:P{"body"}%
%TMPL:P{"htmlend"}%
%TMPL:END%

%{
  Prints the entire document.
  (wrapped in comments to prevent foswiki's theme engine to produce unwanted paragraphs)
}%
%{}%%TMPL:P{"document"}%%{}%

%{ HTML5 doctype }%
%TMPL:DEF{"doctype"}%<!DOCTYPE html>%TMPL:END%
%TMPL:DEF{"htmlstart"}%<html class="no-js" lang="%LANG%">%TMPL:END%
%TMPL:DEF{"htmlend"}%</html>%TMPL:END%

%TMPL:DEF{"head"}%%{
}%<head>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="%TOPIC%">
  <meta name="TEXT_NUM_TOPICS" content="%MAKETEXT{"Number of topics:"}%">
  <meta name="TEXT_MODIFY_SEARCH" content="%MAKETEXT{"Modify search"}%">
  <meta name="robots" content="noindex">
  <link rel="icon" href="%FAVICON%" type="image/x-icon" />
  <link rel="shortcut icon" href="%FAVICON%" type="image/x-icon" />
  <link rel="alternate" href="%SCRIPTURL{edit}%/%BASEWEB%/%BASETOPIC%?t=%GMTIME{"$epoch"}%" type="application/x-wiki" title="edit %BASETOPIC%" />
  %TMPL:P{"script"}%
  %TMPL:P{"windowtitle"}%
  %TMPL:P{"defaultstyle"}%
  %TMPL:P{"JavascriptFiles/foswikiPref"}%
</head>%TMPL:END%

%TMPL:DEF{"context"}%approved%TMPL:END%


%TMPL:DEF{"body"}%<!--body-->
%JQREQUIRE{"blockui, cookie, placeholder, ui::tooltip, ui::autocomplete"}%
<body class="context-%TMPL:P{"context"}%">
%TMPL:P{"topbar"}%
%TMPL:P{"menutoggle"}%
%TMPL:P{"leftbar"}%
%TMPL:P{"rightbar"}%
%TMPL:P{"kvpbar"}%
%TMPL:P{"quicksearch"}%
<div class="qw-page" data-offcanvas>
  <section class="content"><div>%TMPL:P{"content"}%</div></section>
</div>
<script>
jQuery(function($) {
  $(this).qwiki();
});
</script>
</body>
<!--/body-->%TMPL:END%


%TMPL:DEF{"topbar"}%<!--topbar-->
<nav class="qw-top" data-offcanvas>
  <div class="one">
    <div class="breadcrumb"><div>%TMPL:P{"breadcrumb"}%</div></div>
    <div class="search">
      <div class="close" data-offcanvas-toggle="quicksearch">
        <div><i class="icon-arrow-right"></i></div>
      </div>
      <div>%TMPL:P{"topbar:search"}%</div>
    </div>
  </div>
  <div class="two">
    <div class="actions"><div>%TMPL:P{"topbar:topicactions"}%</div></div>
    <div class="toggle"><div>&nbsp;</div></div>
  </div>
</nav>
<!--/topbar-->%TMPL:END%


%TMPL:DEF{"topbar:search"}%<!--topbar:search-->
<div class="qw-quicksearch-form">
  <form class="form" action="%SCRIPTURLPATH{view}%/%WEB%/WebSearch">
    <div class="input">
      <input name="search"
             type="text"
             class="jqUIAutocomplete {source:'%SCRIPTURL%/rest/SolrPlugin/autocomplete?limit=5', delay: 500}"
             placeholder="%MAKETEXT{"search..."}%"
             data-offcanvas-toggle="quicksearch">
    </div>
    <div class="submit">
      <a href="#" class="submit"><i class="icon-search"></i></a>
    </div>
  </form>
</div>
<!--/topbar:search-->%TMPL:END%

%TMPL:DEF{"quicksearch"}%<!--quicksearch-->
<div class="qw-quicksearch" data-offcanvas data-target="quicksearch">
  <div>
  </div>
</div>
<!--/quicksearch-->%TMPL:END%


%TMPL:DEF{"kvpbar"}%<!--kvpbar-->
<div class="qw-kvpbar" data-offcanvas data-target="kvp">
  <div class="close" data-offcanvas-toggle="kvp">
    <div><i class="icon-arrow-right"></i></div>
  </div>
  <div>%TMPL:P{"kvp"}%</div>
</div>
<!--/kvpbar-->%TMPL:END%


%TMPL:DEF{"leftbar"}%<!--leftbar-->
<nav class="qw-leftbar" data-offcanvas data-target="leftbar">
  <section class="buttons">%TMPL:P{"leftbar:buttons"}%</section>
  <section class="menu">
    <div class="links">%TMPL:P{"leftbar:links"}%</div>
    <div class="user"></div>
    <div class="actions"></div>
    <div class="ma-logo">
      <span>Modell Aachen GmbH</span>
    </div>
  </section>
</nav>
<!--/leftbar-->%TMPL:END%


%TMPL:DEF{"rightbar"}%<!--rightbar-->
<div class="qw-rightbar" data-offcanvas data-target="rightbar">
  <span>ALLGEMEIN</span>
  <ul>
    <li><a href="#">FooBar 01</a></li>
    <li><a href="#">FooBar 02</a></li>
    <li><a href="#">FooBar 03</a></li>
    <li><a href="#">FooBar 04</a></li>
    <li><a href="#">FooBar 05</a></li>
    <li><a href="#">FooBar 06</a></li>
    <li><a href="#">FooBar 07</a></li>
    <li><a href="#">FooBar 08</a></li>
    <li><a href="#">FooBar 09</a></li>
    <li><a href="#">FooBar 10</a></li>
  </ul>
  <div style="display: none;">%TMPL:P{"attachments"}%</div>
</div>
<!--/rightbar-->%TMPL:END%

%TMPL:DEF{"menutoggle"}%<!--menutoggle-->
<div class="qw-menutoggle" data-offcanvas>
  <div class="customer-logo">%TMPL:P{"logo"}%</div>
  <div class="toggle" data-offcanvas-toggle="leftbar"><div><i class="icon-menu"></i></div></div>
</div>
<!--/menutoggle-->%TMPL:END%


%TMPL:DEF{"breadcrumb"}%<!--breadcrumb-->
<ul class="qw-breadcrumb" data-breadcrumb>
  <!--breadcrumb:home-->%TMPL:P{"breadcrumb:home"}%<!--/breadcrumb:home-->
  <!--breadcrumb:parentwebs-->%TMPL:P{"breadcrumb:parentwebs"}%<!--/breadcrumb:parentwebs-->
  <!--breadcrumb:parenttopics-->%TMPL:P{"breadcrumb:parenttopics"}%<!--/breadcrumb:parenttopics-->
  <!--breadcrumb:topic-->%TMPL:P{"breadcrumb:topic"}%<!--/breadcrumb:topic-->
</ul>
<!--/breadcrumb-->%TMPL:END%

%TMPL:DEF{"breadcrumb:home"}%<li class="qw-breadcrumb-home"><a href="#" data-dropdown="qw-processmap"><i class="icon-app-processes"></i></a></li>%TMPL:END%
%TMPL:DEF{"breadcrumb:parentwebs"}%%MAWEBLIST%%TMPL:END%
%TMPL:DEF{"breadcrumb:parenttopics"}%%META{"parent" nowebhome="on" prefix="<li class=\"qw-breadcrumb-parent\">" separator="</li><li class=\"qw-breadcrumb-parent\">" suffix="</li>"}%%TMPL:END%
%TMPL:DEF{"breadcrumb:topic"}%%IF{"'%TOPIC%' != 'WebHome'" then="<li class=\"qw-breadcrumb-topic\">[[%TOPIC%][$percntTMPL:P{\"breadcrumb:topic:title\"}$percnt]]</li>" else="<li class=\"qw-breadcrumb-topic\">[[%TOPIC%][TBD. WebHome Text]]</li>"}%%TMPL:END%
%TMPL:DEF{"breadcrumb:topic:title"}%%IF{"'%TOPIC%'/TopicTitle" then="$percntFORMFIELD{\"TopicTitle\"}$percnt" else="$percntTOPIC$percnt"}%%TMPL:END%

%TMPL:DEF{"attachments"}%%META{"attachments"}%%TMPL:END%


%TMPL:DEF{"comments"}%<!--comments-->
%TMPL:P{"comments:adorner"}%
%TMPL:P{"comments:container"}%
<!--/comments-->%TMPL:END%

%TMPL:DEF{"comments:adorner"}%<!--comments:adorner-->
<div id="qw-comment-adorner">%MAKETEXT{"post comment"}%</div>
<!--/comments:adorner-->%TMPL:END%

%TMPL:DEF{"comments:container"}%<!--comments:container-->
<div id="qw-comment-container">%FLATCOMMENTS%</div>
<!--/comments:container-->%TMPL:END%

%TMPL:DEF{"comments:boxes"}%<!--comments:boxes-->
%TMPL:P{"comments:viewbox"}%
%TMPL:P{"comments:editbox"}%
<!--/comments:boxes-->%TMPL:END%

%TMPL:DEF{"comments:viewbox"}%<!--comments:viewbox-->
<div class="qw-comment-viewbox">
  <div class="meta">
    <div class="content">
      <span class="user"></span>
    </div>
    <div class="content">
      <span class="date"></span>
    </div>
  </div>
  <div class="comment">
    <div class="content"></div>
  </div>
  <div class="more">
    <div class="content">
      <a id="qw-comment-more" href="#">%MAKETEXT{"read more"}% <i class="fa fa-chevron-down"></i></a>
      <a id="qw-comment-less" class="hidden" href="#">%MAKETEXT{"less"}% <i class="fa fa-chevron-up"></i></a>
    </div>
  </div>
</div>
<!--/comments:viewbox-->%TMPL:END%

%TMPL:DEF{"comments:editbox"}%<!--comments:editbox-->
<div class="qw-comment-editbox">
  <div class="options">
    <div class="content">
      <input type="radio" name="commentType" value="0" id="qw-cmt-type0"><label for="qw-cmt-type0"><span class="idea left"><i class="fa fa-lightbulb-o fa-lg"></i> %MAKETEXT{"idea"}%</span></label>
    </div>
    <div class="content">
      <input type="radio" name="commentType" value="2" id="qw-cmt-type2"><label for="qw-cmt-type2"><span class="issue right"><i class="fa fa-exclamation fa-lg"></i> %MAKETEXT{"issue"}%</span></label>
    </div>
  </div>
  <div class="text">
    <div class="content">
      <form data-abide>
        <div>
          <textarea id="qw-comment-textarea" cols="30" rows="5" required pattern="^[^\s]$|.+"></textarea>
          <small class="error">%MAKETEXT{"required"}%</small>
        </div>
      </form>
    </div>
  </div>
  <div class="actions">
    <div class="content">
      <a href="#" class="submit">%MAKETEXT{"save"}%</a>
    </div>
    <div class="content">
      <a href="#" class="cancel">%MAKETEXT{"cancel"}%</a>
    </div>
  </div>
</div>
<!--/comments:editbox-->%TMPL:END%




