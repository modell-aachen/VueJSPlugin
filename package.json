{
  "name": "VueJSPlugin",
  "version": "1.0.0",
  "description": "VueJSPlugin",
  "main": "pub/System/VueJSPlugin/VueJSPlugin.js",
  "scripts": {
    "build": "webpack -d --watch --config development.webpack.config.js --progress",
    "build-production": "webpack -p --config production.webpack.config.js",
    "build-all": "webpack -p --config production.webpack.config.js && webpack -d --config development.webpack.config.js",
    "build-test-library": "webpack -d --config unit-test-library.webpack.config.js --progress",
    "test": "jest",
    "lint": "eslint --ext .js,.vue dev/ tests/",
    "lint:watch": "watch --color 'eslint --color --ext .js,.vue dev/ tests/'"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue",
      "ts"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "^vue$": "vue/dist/vue.js"
    },
    "testRegex": "Spec\\.js$",
    "transformIgnorePatterns": [
      "node_modules/(?!(vue-timers)/)"
    ],
    "collectCoverageFrom": [
      "dev/**/*.{js,vue}",
      "!**/node_modules/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@git.modac.lan:q-wiki/VueJSPlugin.git"
  },
  "author": "Modell Aachen GmbH",
  "license": "GPL-3.0",
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.5.2",
    "babel-runtime": "^6.23.0",
    "css-loader": "^1.0.0",
    "eslint": "^4.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^4.3.0",
    "file-loader": "^0.11.2",
    "jasmine": "^2.5.2",
    "jasmine-ajax": "^3.2.0",
    "jquery": "^3.2.1",
    "js-base64": "^2.3.2",
    "json-loader": "^0.5.4",
    "moment": "^2.18.1",
    "node-sass": "^4.5.1",
    "sass-loader": "^7.1.0",
    "sass-rem": "^2.0.1",
    "style-loader": "^0.23.0",
    "url-loader": "^0.5.7",
    "v-tooltip": "^2.0.0-rc.1",
    "vue": "^2.5.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.3.4",
    "vuex": "^2.2.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@vue/test-utils": "^1.0.0-beta.22",
    "@websanova/vue-upload": "^1.0.2-beta",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-es2017-object-entries": "^0.0.4",
    "formdata-polyfill": "^3.0.11",
    "i18next": "^10.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "nprogress": "^0.2.0",
    "ts-jest": "^23.10.2",
    "ts-loader": "^5.2.1",
    "typescript": "^3.0.3",
    "v-infinite-scroll": "^1.0.4",
    "vddl": "^0.7.1",
    "vee-validate": "2.1.0-beta.1",
    "vue-click-outside": "^1.0.7",
    "vue-i18next": "^0.9.1",
    "vue-jest": "^2.6.0",
    "vue-params": "^0.9.2",
    "vue-resource": "^1.5.0",
    "vue-router": "^3.0.1",
    "vue-slide-up-down": "git://github.com/modell-aachen/vue-slide-up-down.git",
    "vue-sweetalert": "^0.1.18",
    "vue-timers": "^1.7.3",
    "vue-visible": "^1.0.2"
  }
}
